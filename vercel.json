{
  "version": 2,
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NODE_VERSION": "20",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "app/**/*.ts": {
      "runtime": "nodejs20.x"
    },
    "app/**/*.js": {
      "runtime": "nodejs20.x"
    }
  }
}
